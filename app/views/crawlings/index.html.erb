<h1>Crawlings#index</h1>

<h1>Listing URL</h1>

<table>
  <thead>
    <tr>
      <th>URL</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crawlings.each do |c| %>
      <tr>
        <td><%= c.url %></td>
        <td><%= link_to 'Show', crawling_path(c.id) %></td>
        <td><%= link_to 'Edit', edit_crawling_path(c.id) %></td>
        <td><%= link_to 'Destroy', crawling_path(c.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crawling', new_crawling_path %>
